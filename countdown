#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int n;
	cout<<"Enter number: "<<endl;
	cin>>n;
	for(int i=n;i>=1;i--){
	cout<<" "<<i<<endl;
		if(i%5==0){
			cout<<"Beep"<<endl;
		}
	}
	return 0;
}
